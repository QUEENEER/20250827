#include <stdio.h>
void su(int a,int arr[]){
	scanf("%d",&a);
	for(int i=0;i<a;i++){
		//TODO
		if(0!=a%i){
			for(int j=0;j<a;j++){
				arr[j]=i;
			}
		}
	}
}
void yu(int a,int arr[]){
	int m=0;
	for(int j=0;j<a;j++){
		//TODO
		for(int i=0;i<a/2;i++){
			//TODO
			if(arr[j]+arr[i]==a){
				//TODO
				m=m+1;
			}
		}
	}
	printf("%d",m);
}

int main()


{
	int a=0;
	int arr[100000];
	su(a,arr);
	yu(a,arr);
	
	return 0;
}
//change
#include <stdio.h>
int su(int *a,int arr[]){
	scanf("%d",a);
	int idx=0;
	for(int i=2;i<*a;i++){
		int ifprime=1;
		for(int j=2;j*j<=i;j++){
			if(i%j==0){
				ifprime=0;
				break;
			}
		}
		if(ifprime){
			arr[idx++]=i;
		}
	}
	return idx;
}
void yu(int a,int arr[],int primecut){
	int m=0;
	for(int i=0;i<primecut;i++){
		//TODO
		for(int j=i;j<primecut;j++){
			if(arr[i]+arr[j]==a){
				m++;
			}
		}
	}
	printf("%d",m);
}

int main()


{
	int a=0;
	int arr[100000];
	int primecut=su(&a,arr);
	yu(a,arr,primecut);
	
	return 0;
}
