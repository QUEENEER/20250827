#include <stdio.h>


int main()

{
	int n;
	int arr[100];
	int m=0;
	scanf("%d",&n);
	for(int i=0;i<n;i++){
		scanf("%d",&arr[i]);
	}
	for(int i=0;i<n;i++){
		for(int j=3;j++;){
			if(arr[i]%j==0){
				m=m+1;
			}
		}
	}
	printf("%d",m);
	return 0;
}
