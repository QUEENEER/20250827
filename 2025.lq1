#include <stdio.h>


int main()

{
	int n;
	int arr[n];
	int m=0;
	scanf("%d",&n);
	for(int i=0;i<n;i++){
		scanf("%d",&arr[i]);
	}
	for(int i=0;i<n;i++){
		if(arr[i]%3==0){
			m=m+1;
		}
	}
	printf("%d",m);
	return 0;
}
